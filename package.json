{
  "name": "graasp-research",
  "version": "0.1.11",
  "private": true,
  "author": "React EPFL",
  "contributors": [
    "Hagop Taminian",
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang"
  ],
  "dependencies": {
    "@graasp/query-client": "github:graasp/graasp-query-client",
    "@graasp/ui": "github:graasp/graasp-ui",
    "@graasp/utils": "github:graasp/graasp-utils",
    "@material-ui/core": "4.9.13",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.52",
    "google-map-react": "1.1.7",
    "i18next": "19.9.2",
    "immutable": "4.0.0",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "qs": "6.10.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-ga": "3.3.0",
    "react-i18next": "11.15.3",
    "react-router-dom": "6.2.1",
    "react-scripts": "5.0.0",
    "react-select": "5.2.2",
    "react-toastify": "8.2.0",
    "recharts": "1.8.5",
    "supercluster": "7.1.4",
    "use-supercluster": "0.2.9"
  },
  "scripts": {
    "pre-commit": "yarn prettier:check && yarn lint",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f ./.env.dev yarn build",
    "dist": "env-cmd -f ./.env.prod yarn build",
    "test": "react-scripts test",
    "test:once": "cross-env CI=true env-cmd -f ./.env.test react-scripts test",
    "eject": "react-scripts eject",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "lint": "eslint .",
    "prettier:check": "prettier --check 'src/**/*.js'",
    "prettier:write": "prettier --write 'src/**/*.js'",
    "release": "git fetch --tags && git add CHANGELOG.md && standard-version -a",
    "release:first": "git fetch --tags && git add CHANGELOG.md && standard-version -a --first-release",
    "postinstall": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@babel/eslint-plugin": "7.16.5",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "cross-env": "7.0.3",
    "env-cmd": "10.1.0",
    "eslint": "8.9.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "husky": "7.0.0",
    "prettier": "2.5.1",
    "standard-version": "8.0.2",
    "typescript": "4.5.5"
  },
  "packageManager": "yarn@3.2.0"
}
