    "@graasp/translations": "1.19.4",
    "@graasp/ui": "4.0.0",
    "@mui/icons-material": "5.14.6",
    "@mui/lab": "5.0.0-alpha.141",
    "@mui/material": "5.14.6",
    "@sentry/react": "7.72.0",
    "google-map-react": "2.2.1",
    "http-status-codes": "2.3.0",
    "i18next": "23.6.0",
    "lodash.capitalize": "4.2.1",
    "lodash.countby": "4.6.0",
    "lodash.groupby": "4.6.0",
    "lodash.sortby": "4.7.0",
    "lodash.truncate": "4.4.2",
    "miragejs": "0.1.47",
    "qs": "6.11.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-ga4": "2.1.0",
    "react-i18next": "13.3.1",
    "react-router-dom": "6.15.0",
    "react-toastify": "9.1.3",
    "recharts": "2.8.0",
    "stylis": "4.3.0",
    "stylis-plugin-rtl": "2.1.1",
    "supercluster": "8.0.1",
    "use-supercluster": "0.4.0",
    "uuid": "9.0.1"
  },
  "scripts": {
    "dev": "vite",
    "start": "yarn dev",
    "start:test": "vite --mode test",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode test",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "preview:test": "vite preview --mode test",
    "lint": "eslint .",
    "pre-commit": "yarn prettier:check && yarn lint && yarn type-check",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "type-check": "tsc --noEmit && tsc --noEmit -p cypress/tsconfig.json",
    "check": "yarn prettier:check && yarn lint && yarn type-check",
    "test": "yarn build:test && concurrently -k -s first \"yarn preview:test\" \"yarn cypress:run\"",
    "cypress": "yarn test",
    "cypress:open": "env-cmd -f ./.env.test cypress open --browser chrome",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome",
    "prettier:check": "prettier --check 'src/**/*.{js,tsx,ts}'",
    "prettier:write": "prettier --write 'src/**/*.{js,tsx,ts}'",
    "postinstall": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@cypress/code-coverage": "3.12.1",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.5.1",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/google-map-react": "2.1.7",
    "@types/jest": "29.5.3",
    "@types/lodash.capitalize": "4.2.7",
    "@types/lodash.countby": "4.6.8",
    "@types/lodash.groupby": "4.6.8",
    "@types/lodash.truncate": "4.4.7",
    "@types/node": "18.18.0",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vitejs/plugin-react": "4.1.0",
    "cross-env": "7.0.3",
    "cypress": "12.17.4",
    "env-cmd": "10.1.0",
    "eslint": "8.50.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "husky": "8.0.3",
    "istanbul-lib-coverage": "3.2.0",
    "nyc": "15.1.0",
    "prettier": "3.0.3",
    "rollup-plugin-visualizer": "5.9.2",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-checker": "0.6.2",
    "vite-plugin-istanbul": "5.0.0"
  },
  "packageManager": "yarn@3.6.3"
}
